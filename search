<!--下拉菜单-->
<script type="text/javascript">
//模拟下拉菜单
function tab_search_btn(type){
if(type==2){
	$('#all_search').fadeOut();$('#fast_search').fadeIn();
	$('#shouqi').hide();
}else{
	$('#all_search').fadeIn();$('#fast_search').fadeOut();
	$('#shouqi').show();
}
}
            (function ($) {
                $.fn.jQSelect = function (settings) {

                    var $div = this;
                    var $cartes = $div.find(".select-cate");
                    var $lists = $div.find(".select-con");

                    var listTxt = $cartes.find(".selectTxt");
                    var listVal = $cartes.find(".selectVal");

                    var items = $lists.find("ul > li");

                    $div.hover(function () {
                        $(this).addClass("select-down");
                    }, function () {
                        $(this).removeClass("select-down");
                    });

                    //绑定点击事件
                    items.click(function () {
                        listVal.val($(this).attr("data"));
                        listTxt.val($(this).text());
						window.location.href="http://www.guifun.com/newhouse/search/?sc="+listVal.attr("data-key")+$(this).attr("data");
                        $div.removeClass("select-down");
                    }).mouseover(function () {
                        $(this).removeClass("cwhite");
                        $(this).addClass("cgrey");
                    }).mouseout(function () {
                        $(this).removeClass("cgrey");
                        $(this).addClass("cwhite");
                    });

                };
            })(jQuery);
</script>
<script type="text/javascript">
    jQuery(document).ready(function () {
 
        jQuery("#select04").jQSelect({});
        jQuery("#select05").jQSelect({});
        jQuery("#select06").jQSelect({});
        jQuery("#select07").jQSelect({});
		jQuery("#select08").jQSelect({});
    });

</script>
