<script>
function autoListbox(appendTo){
	var html = '<dl class="cpCarSec"><dd><select name="data[qyid]" id="xqy" onchange="enListbox()"><option value="0">请选择</option><option value="9">其他</option>';
    $.ajax({type:"post",url:"http://www.guifun.com/newhouse/ajaxarea/",data:"",dataType:"json",
       success:function(data){
       	var json = Array();
       	json = data;
       	for(var i=0; i<json.length; i++){
       		html +="<option value=\""+json[i].id+"\">"+json[i].name+"</option>";
       	}
        html +='</select></dd>';
        html +='<dd><select id="xfloor" name="data[floorid]" onchange="enSetbox()">';
        html +='<option value="0">请选择</option></select></dd></dl><input id="xname" type="hidden" name="data[floorname]"/>';
        $("#"+appendTo).html(html);
       }
    });
}
//
//
function enListbox(){
    var oListbox = document.getElementById("xqy");
    var offset =  oListbox.selectedIndex;
    var offsetValue = oListbox.options[offset].getAttribute("value");
    if(offsetValue==0) return false;
    var oS = document.getElementById("xfloor");
    var nums = oS.options.length;
    for(var j=nums; j>0; j--){ oS.remove(j);}

    $.ajax({type:"post",url:"http://www.guifun.com/newhouse/ajax/",data:"id="+offsetValue,dataType:"json",
     success:function(data){
        var json = Array();
        json = data;
        for(var i=0; i<json.length; i++){
            var oSelect = document.getElementById("xfloor");
            var op = document.createElement("option");
            op.appendChild(document.createTextNode(json[i].house_title));
            op.setAttribute("value",json[i].house_id+'||'+json[i].house_title+'||均价'+json[i].average_price_new+'元/平方米');
            oSelect.appendChild(op);
        }
    }
  })
}
function enSetbox(){
    var oListbox = document.getElementById("xfloor");
    var offset =  oListbox.selectedIndex;
    var offsetValue = oListbox.options[offset].value;
    if(offsetValue) {
	    var temp = offsetValue.split('||');	
	    var str = '<li style="display: block;" id="comparelinum'+temp[0]+'"><h3><a href="http://www.guifun.com/house/{0}.html'+temp[0]+'" target="_blank">'+temp[1]+'</a></h3><span class="price">'+temp[2]+'</span><a href="javascript:"  onclick="compareclose('+temp[0]+')" class="compareclose close" rel="'+temp[0]+'" title="移除"></a></li>';
        var oldstr = $("#compareUl").html();
		
		var num = $("#compareUl > li").size();
		if(num>3 && $("#gfcheckbox"+temp[0]).attr("checked")=="checked") {
			$(this).attr("checked",false);
			alert('最多选择4个楼盘');
			return true;
		}     
		
	    var tt = new Array();	
        var is = true;
        var cookie_new_gfcompare = '';	
        var cookie_old_gfcompare_value = $.cookie('gfcompare');	
        var cookie_old_gfcompare = new Array();
        if(cookie_old_gfcompare_value) {
            cookie_old_gfcompare = cookie_old_gfcompare_value.split('[-]');	             
        }		
		if($("#gfcheckbox"+temp[0]).attr("checked")!="checked") {
			if(num<=3) {
				$("#compareUl").html(oldstr+str);		
			}			
            for(var i=0;i<cookie_old_gfcompare.length;i++) {
                tt = cookie_old_gfcompare[i].split('||');
				if(tt[0] && tt[1] && tt[2]) {
					if(tt[0]==temp[0]){
						is = false;
						continue;
					}
					cookie_new_gfcompare += tt[0]+'||'+tt[1]+'||'+tt[2]+'[-]' ;				
				}
            } 	
            if(is==true) {
			    $.cookie('gfcompare', cookie_new_gfcompare + temp[0]+'||'+temp[1]+'||'+temp[2]+'[-]',{ path: '/', expires: 10 });
            }else {
			    $.cookie('gfcompare', cookie_new_gfcompare,{ path: '/', expires: 10 });
            }	
            $("#Jchoose").html(num+1);			
			if(num>=3){
				$("#compareFaker").css("display","none");
			}else {
				$("#compareFaker").css("display","block");				
			}	
	    }
		
	
	}
}
autoListbox("ajaxbox");
</script> 
