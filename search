<!-- 报名 end-->
   <script>
//==验证表单内容必须填写
function tuan_checkForm(){
	
	jQuery('input[require]').removeClass("error");
	
	var ok = true;
	var msg = '';	
 //验证 input
	jQuery.each(jQuery('input[require]'),function(index,obj){
		obj = jQuery(obj);
		var reg = obj.attr('regexp'); 
		if(reg) { 
			var value=jQuery.trim(obj.val());
			 obj.removeClass("error");	 
			if(new RegExp(reg,"gi").test(value)==false){
				obj.addClass("error");
				ok = ok && false;
				if(obj.attr("msg")){
				msg = obj.attr("msg");
				}
				
			}  
		}
	})
	//==

	if(!ok){ alert(msg);return false;}
	return ok;
	 
}
	function ajaxtuan(){ 
		 if(tuan_checkForm()){
		 var poststr = $("#GFtuanform").serialize();
		 poststr += '&bz[]=我要抢优惠报名'+"&bz[]=";
		 
			var post_url = 'http://www.guifun.com/subject/ajaxpost/';
			 var params = "subid=137&"+poststr+"&rnd="+Math.random();
				$.ajax({
					type:"post",
					url: post_url,
					data:params,
                                        dataType: "jsonp",
                                        jsonp: "jsonpcallback",
					success:function(data){
			            data = $.trim(data);
						
						if(data == 'x1'){
						   alert("报名成功！请留意接听桂房网客服电话！");
						   $("#GFtuanform")[0].reset();
						   
						}else{
							alert("请填写完整的资料以便我们更好的为您提供服务！"+data);
						}
					}
				});	
		} 
	}
</script>
