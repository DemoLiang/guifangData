<script>

	function compareclose(tvalue) {
 	    var num = $("#compareUl > li").size();
		//var tvalue = $(this).attr("rel");
        var tt = new Array();	
        var is = true;
        var cookie_new_gfcompare = '';	
        var cookie_old_gfcompare_value = $.cookie('gfcompare');	
        var cookie_old_gfcompare = new Array();
        if(cookie_old_gfcompare_value) {
            cookie_old_gfcompare = cookie_old_gfcompare_value.split('[-]');	             
        }
		for(var i=0;i<cookie_old_gfcompare.length;i++) {
			tt = cookie_old_gfcompare[i].split('||');
			if(tt[0] && tt[1] && tt[2]) {
				if(tt[0]==tvalue){
					is = false;
					continue;
				}
				cookie_new_gfcompare += tt[0]+'||'+tt[1]+'||'+tt[2]+'[-]' ;				
			}
		} 	
		$.cookie('gfcompare', cookie_new_gfcompare,{ path: '/', expires: 10 });
		$("#comparelinum"+tvalue).remove();	
		$("#gfcheckbox"+tvalue).attr("checked",false);	
        $("#Jchoose").html(num-1);		
		if(num>4){
			$("#compareFaker").css("display","none");
		}else {
			$("#compareFaker").css("display","block");				
		}
    }	
		
$(function(){
    cominit();
    function cominit(){
		var tt = new Array();
        var num =0;		
		var gfcompare_ul_li = '';	
		var cookie_old_gfcompare_value = $.cookie('gfcompare');	
		//alert(cookie_old_gfcompare_value);
		var cookie_old_gfcompare = new Array();
		if(cookie_old_gfcompare_value) {
			cookie_old_gfcompare = cookie_old_gfcompare_value.split('[-]');	
			for(var i=0;i<cookie_old_gfcompare.length;i++) {
				tt = cookie_old_gfcompare[i].split('||');
				if(tt[0] && tt[1] && tt[2]) {
				    num ++;
					$("#gfcheckbox"+tt[0]).attr("checked","checked");
					gfcompare_ul_li +=  '<li style="display: block;" id="comparelinum'+tt[0]+'"><h3><a href="http://www.guifun.com/house/{0}.html'+tt[0]+'" target="_blank">'+tt[1]+'</a></h3><span class="price">'+tt[2]+'</span><a href="javascript:" onclick="compareclose('+tt[0]+')" class="compareclose close" rel="'+tt[0]+'"   title="移除"></a></li>';
				}
			}
			if(gfcompare_ul_li) {
				$("#compareUl").html(gfcompare_ul_li);
			}
			$("#Jchoose").html(num);
			if(num>3){
				$("#compareFaker").css("display","none");
			}else {
				$("#compareFaker").css("display","block");				
			}					
		}
		if(num>0){
			$("#cbicon_body").css("display","block");
            $("#JcompareBar").css("display","block");			
		}else{
			$("#cbicon_body").css("display","none");
            $("#JcompareBar").css("display","none");			
		}		
	}	

    $(".doselect").click( function () {	
	    var num = $("#compareUl > li").size();
		if(num>3 && $(this).attr("checked")=="checked") {
		    $(this).attr("checked",false);
		    alert('最多选择4个楼盘');
			return true;
		}
		var tvalue = $(this).attr("value");
		var tdoshname = $(this).attr("doshname");
		var tdosprice = $(this).attr("dosprice");	
	    var str = '<li style="display: block;" id="comparelinum'+tvalue+'"><h3><a href="http://www.guifun.com/house/{0}.html'+tvalue+'" target="_blank">'+tdoshname+'</a></h3><span class="price">'+tdosprice+'</span><a href="javascript:"  onclick="compareclose('+tvalue+')" class="compareclose close" rel="'+tvalue+'" title="移除"></a></li>';
        var oldstr = $("#compareUl").html();
		if($("#compareUl").children().is($("#comparelinum"+tvalue))) {
			$(this).attr("checked",'checked');
		    alert('你已经选择了该楼盘了');
			return true;
		}		
        var tt = new Array();	
        var is = true;
        var cookie_new_gfcompare = '';	
        var cookie_old_gfcompare_value = $.cookie('gfcompare');	
        var cookie_old_gfcompare = new Array();
        if(cookie_old_gfcompare_value) {
            cookie_old_gfcompare = cookie_old_gfcompare_value.split('[-]');	             
        }	

		$("#cbicon_body").css("display","block");	
        $("#JcompareBar").css("display","block");		
       // alert(""+cookie_old_gfcompare_value);
		if($(this).attr("checked")=="checked") {
			if(num<=3) {
				$("#compareUl").html(oldstr+str);		
			}	
            for(var i=0;i<cookie_old_gfcompare.length;i++) {
                tt = cookie_old_gfcompare[i].split('||');
				if(tt[0] && tt[1] && tt[2]) {
					if(tt[0]==tvalue){
						is = false;
						continue;
					}
					cookie_new_gfcompare += tt[0]+'||'+tt[1]+'||'+tt[2]+'[-]' ;				
				}
            } 	
            if(is==true) {
			    $.cookie('gfcompare', cookie_new_gfcompare + tvalue+'||'+tdoshname+'||'+tdosprice+'[-]',{ path: '/', expires: 10 });
            }else {
			    $.cookie('gfcompare', cookie_new_gfcompare,{ path: '/', expires: 10 });
            }	
            $("#Jchoose").html(num+1);			
			if(num==3){
				$("#compareFaker").css("display","none");
			}else {
				$("#compareFaker").css("display","block");				
			}			
	    }else {		
            $("#comparelinum"+tvalue).remove();
            for(var i=0;i<cookie_old_gfcompare.length;i++) {
                tt = cookie_old_gfcompare[i].split('||');
				if(tt[0] && tt[1] && tt[2]) {
					if(tt[0]==tvalue){
						is = false;
						continue;
					}
					cookie_new_gfcompare += tt[0]+'||'+tt[1]+'||'+tt[2]+'[-]' ;				
				}
            } 	
			$.cookie('gfcompare', cookie_new_gfcompare,{ path: '/', expires: 10 });	
            $("#Jchoose").html(num-1);			
			if(num>4){
				$("#compareFaker").css("display","none");
			}else {
				$("#compareFaker").css("display","block");				
			}				
		}
		
	 });		


