				if(tt[0] && tt[1] && tt[2]) {
					gfcompare_hoids +=  tt[0]+',';
				}
			}
			if(gfcompare_hoids) {	
                gfcompare_hoids = '&hoid='+gfcompare_hoids;			
                $(this).attr('href','http://www.guifun.com/newhouse/compare/?'+gfcompare_hoids);	
			}		
		}		
	});		 
