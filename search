<script type="text/javascript">
//模拟下拉菜单
 
            (function ($) {
                $.fn.jQSelect2 = function (settings) {

                    var $div = this;
                    var $cartes = $div.find(".select-view");
                    var $lists = $div.find(".select-options");

                    var listTxt = $cartes.find(".selectTxt");
                    var listVal = $cartes.find(".selectVal");
                    var items = $lists.find("ul > li");
                    $div.hover(function () {
                        $(this).addClass("select-down");
                    }, function () {
                        $(this).removeClass("select-down");
                    });

                    //绑定点击事件
                    items.click(function () {
                        listVal.val($(this).attr("data"));
                        listTxt.html($(this).text());
						var curr = "";
						var sel_key = listVal.attr("data-key");
						if(curr && curr.indexOf(sel_key)>-1){
							var regx = new RegExp(""+sel_key+"\\d+","gim");
							curr = curr.replace(regx, sel_key+$(this).attr("data"));
						}else{
							curr =curr+listVal.attr("data-key")+$(this).attr("data");
						}
				window.location.href="http://www.guifun.com/newhouse/search/?sc="+curr;
				//+listVal.attr("data-key")+$(this).attr("data");
                        $div.removeClass("select-down");
                    }).mouseover(function () {
                        $(this).removeClass("cwhite");
                        $(this).addClass("cgrey");
                    }).mouseout(function () {
                        $(this).removeClass("cgrey");
                        $(this).addClass("cwhite");
                    });

                };
            })(jQuery);
</script>
<script type="text/javascript">
function oversh(id){
	if(id=="show"){
		$("#ewm").show();
	}else{
		$("#ewm").hide();
	}
}
